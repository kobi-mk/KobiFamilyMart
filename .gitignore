# Ignore node_modules in backend and frontend
node_modules/
frontend/node_modules/

# Ignore environment variables
.env
frontend/.env

# Ignore logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore build outputs except for frontend/build
build/
!frontend/build/

# Ignore coverage folder
coverage/
frontend/coverage/

# Ignore DS_Store (macOS)
.DS_Store

# Ignore IDE specific files (optional)
.vscode/
.idea/
*.iml

# Ignore yarn.lock or package-lock.json if not using it
yarn.lock
frontend/yarn.lock
package-lock.json
frontend/package-lock.json

# Ignore any generated files by npm or yarn
.pnpm-debug.log*
.yarn-integrity
frontend/.yarn-integrity
.yarnrc.yml
frontend/.yarnrc.yml
